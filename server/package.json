    "jsonwebtoken": "8.3.0",
    "prisma-binding": "2.1.1"
  },
  "devDependencies": {
    "dotenv": "6.0.0",
    "graphql-cli": "2.16.5",
    "nodemon": "1.18.3",
    "npm-run-all": "4.1.3",
    "prisma": "1.13.2"
  }
}
